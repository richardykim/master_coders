<%= form_for @experiment do |f| %>
<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :description %>
  <%= f.text_area :description %>
</p>

<p>
  <%= f.label :results %>
  <%= f.text_area :results %>
</p>

<p>
  <%= f.label :conclusions %>
  <%= f.text_area :conclusions %>
</p>

<p>
  <%= f.submit %>
</p>

<% end %>
