<!DOCTYPE html>
<html>
  <head>
    <title>CookiesAndCream</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if logged_in?%>
      <%= link_to "Log out!", users_logout_path%>
    <%else%>
      <%= link_to "Register!", new_user_path%>
      <%= link_to "Log in!", users_login_path%>
    <%end%>
    <%= yield %>
  </body>
</html>
